{
    "extends": "airbnb",
    "rules": {
      "prefer-destructuring": [
        "error",
        { "array": false, "object": true },
        { "enforceForRenamedProperties": true }
      ]
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "mocha": true
    }
}
